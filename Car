class Vehicle:
    LicenseNumber = ""
    serialCode = ""
    face = ""
    def turnOnAirConditioner(self):
        print("Turn On : Air")
class Car(Vehicle):
    def sayHello(self):
        print("Hello World")
class PickUp(Vehicle):
    pass
class Van(Vehicle):
    pass
class EstateCar(Vehicle):
    pass
pickUp1 = Car()
pickUp1.turnOnAirConditioner()
pickUp2 = PickUp()
pickUp2.turnOnAirConditioner()
pickUp3 = Van()
pickUp3.turnOnAirConditioner()
pickUp4 = EstateCar()
pickUp4.turnOnAirConditioner()
